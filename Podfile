platform :ios, '9.0'
inhibit_all_warnings!
xcodeproj 'FanRACMVVM'

target 'FanRACMVVM' do
    # 快捷工具库
    pod 'IQKeyboardManager', '>= 6.2.0'

    # FRP & MVVM
    #8.0.2 swift
    pod 'ReactiveCocoa', '>= 8.0.2'
    pod 'ReactiveViewModel', '>= 0.3'
    pod 'AFNetworking-RACExtensions', '>= 0.1.8'
    pod 'RACAutoDisposer', '>= 0.2.0'
    
    # 模型&数据
    pod 'JSONModel', '>= 1.8.0'
    pod 'JSONModel-RACExtensions', '>= 0.2.1'
    pod 'SBJson', '~> 5.0.0'
    
    # Animate
    pod 'POP+MCAnimate', '>= 2.0.1'
    pod 'MBProgressHUD', '>= 1.1.0'
end
